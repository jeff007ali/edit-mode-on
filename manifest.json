{
    "name": "InstaEdit",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": ["activeTab"],
    "browser_action": {
        "default_title": "InstaEdit",
        "default_icon": "icon.png"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    }
}